job('seed') {
    scm {
        github ''
    }
    triggers {
        scm 'H/5 * * * *'
    }
    steps {
        gradle 'clean test'
        dsl {
            external 'jobs/*Jobs.groovy'
            additionalClasspath '${project.projectDir}/src/main/groovy'
        }
    }
    publishers {
        archiveJunit 'build/test-results/**/*.xml'
    }
}